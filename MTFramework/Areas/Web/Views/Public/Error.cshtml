<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
@{
    ViewBag.Title = "操作失败";
    Layout = null;
}
<head>
    @MT.Common.ResourceHelper.CreateStyleResourceUrl("Admin", new[] { "common", "backstage" })
</head>
<body>
    <div class="m-opersf f-cb">
        <dl>
            <dt class="ico-fail"></dt>
            <dd>
                <h1 class="z-err">
                    @ViewData["info"]</h1>
                <p>
                    将在<em class="z-err">@ViewData["time"].ToString()</em>秒后自动跳转，若不想等待，请<a href="javascript:history.go(-1);">点击此处</a>跳转</p>
                <p>
                    <i class="z-err"></i>
                </p>
            </dd>
        </dl>
    </div>
    <script type="text/javascript">
    setTimeout(function () {
        history.go(-1);
    }, @Convert.ToInt32(ViewData["time"])*1000 );
    </script>
</body>
</html>
